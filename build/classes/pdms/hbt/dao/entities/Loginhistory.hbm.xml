<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pdms.hbt.dao.entities.Loginhistory" table="LOGINHISTORY" schema="pdms">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="loginid" type="java.lang.String">
            <column name="loginid" length="20" not-null="true" />
        </property>
        <property name="loginnm" type="java.lang.String">
            <column name="loginnm" length="20" not-null="true" />
        </property>
        <property name="logindt" type="java.sql.Timestamp">
            <column name="logindt" length="23" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
