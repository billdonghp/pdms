<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pdms.hbt.dao.entities.Rmtb" table="RMTB" schema="pdms">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="rid" type="java.lang.String">
            <column name="RID" length="10" not-null="true" />
        </property>
        <property name="mid" type="java.lang.Integer">
            <column name="MID" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
