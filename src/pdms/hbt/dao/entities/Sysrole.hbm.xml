<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pdms.hbt.dao.entities.Sysrole" table="SYSROLE" schema="pdms">
        <id name="id" type="java.lang.String">
            <column name="id" length="10" />
            <generator class="assigned" />
        </id>
        <property name="rolenm" type="java.lang.String">
            <column name="rolenm" length="30" not-null="true" />
        </property>
        <property name="rolerm" type="java.lang.String">
            <column name="rolerm" length="50" />
        </property>
    </class>
</hibernate-mapping>
